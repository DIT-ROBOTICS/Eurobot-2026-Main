<root BTCPP_format="4">
    <TreeNodesModel>
        <Action ID="CamReceiver"/>
        <Action ID="DecisionCore">
            <output_port name="next_target_index" type="int"/>
            <output_port name="next_target_pose" type="geometry_msgs::msg::PoseStamped"/>
            <output_port name="action_type" type="std::string"/>
        </Action>
        <Action ID="NavigationActionNode">
            <output_port name="final_pose" type="geometry_msgs::msg::PoseStamped"/>
            <input_port name="goal" type="geometry_msgs::msg::PoseStamped"/>
            <input_port name="dock_type" type="std::string"/>
        </Action>
        <Action ID="Docking">
            <output_port name="final_pose" type="geometry_msgs::msg::PoseStamped"/>
            <input_port name="base" type="geometry_msgs::msg::PoseStamped"/>
            <input_port name="offset" type="double"/>
            <input_port name="shift" type="double"/>
            <input_port name="dock_type" type="std::string"/>
            <input_port name="isPureDocking" type="bool"/>
        </Action>
        <Action ID="RotateActionNode">
            <output_port name="final_pose" type="geometry_msgs::msg::PoseStamped"/>
            <input_port name="base" type="geometry_msgs::msg::PoseStamped"/>
            <input_port name="degree" type="double"/>
        </Action>
        <Action ID="StopRobotNode"/>
    </TreeNodesModel>
</root>
